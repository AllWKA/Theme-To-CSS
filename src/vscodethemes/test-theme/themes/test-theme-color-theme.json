      ],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "scope": [
        "variable.language"
      ],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "scope": [
        "meta.preprocessor"
      ],
      "settings": {
        "foreground": "#C586C0"
      }
    },
    {
      "scope": [
        "comment",
        "comment.line",
        "comment.block",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6A9955",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant",
        "constant.numeric",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#B5CEA8"
      }
    },
    {
      "scope": [
        "entity",
