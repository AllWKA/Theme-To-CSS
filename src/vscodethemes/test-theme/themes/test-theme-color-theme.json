      "settings": {
        "foreground": "#B5CEA8",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "entity.name.namespace",
        "entity.name.package"
      ],
      "settings": {
        "foreground": "#B8D7A3"
      }
    },
    {
      "scope": [
        "entity.name.enum",
        "entity.name.enum.member"
      ],
      "settings": {
        "foreground": "#B8D7A3"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "scope": [
        "meta.function-call",
        "support.function.builtin"
      ],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "scope": [
        "meta.method-call",
        "entity.name.function.member"
      ],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "scope": [
        "variable.other.readwrite",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "scope": [
        "variable.other.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "scope": [
        "markup.raw",
        "markup.inline.raw",
        "markup.raw.block"
      ],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#C586C0"
      }
    },
    {
      "scope": [
        "punctuation.definition.string",
        "punctuation.definition.variable"
      ],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": [
        "storage.type.class",
        "storage.type.interface",
        "storage.type.struct"
      ],
      "settings": {
        "foreground": "#B8D7A3",
        "fontStyle": "bold"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#ff0000",
    "type": "#ff4500",
    "class": {
      "foreground": "#00ff00",
      "italic": true
    },
    "interface": "#00ced1",
    "enum": "#9932cc",
    "function": {
      "foreground": "#1e90ff",
      "bold": true
    },
    "method": "#1e90ff",
    "macro": "#ff1493",
    "variable": "#ffa500",
    "parameter": "#ffd700",
    "property": "#32cd32",
    "enumMember": "#7fff00",
    "event": "#00fa9a",
    "*.declaration": {
      "bold": true
    },
    "*.definition": {
      "italic": true
    },
    "*.readonly": "#ff4500",
    "*.static": {
      "underline": true
    },
    "*.deprecated": {
      "foreground": "#808080",
      "italic": true,
      "strikethrough": true
    },
    "*.abstract": {
      "fontStyle": "italic"
    },
    "*.async": {
      "foreground": "#1e90ff",
      "bold": true
    },
    "*.modification": {
      "foreground": "#ffa500"
    },
    "*.documentation": {
      "foreground": "#808080",
      "italic": true
    },
    "*.defaultLibrary": {
      "foreground": "#a9a9a9"
    },
    "variable.readonly:javascript": "#ff6347",
    "class:java": {
      "foreground": "#32cd32",
      "italic": true
    },
    "function.declaration": {
      "foreground": "#1e90ff",
      "bold": true,
      "underline": true
    },
    "property.readonly" : "#8a2be2"
  }
}
